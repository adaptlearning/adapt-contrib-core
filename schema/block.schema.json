{
  "$anchor": "block",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "$merge": {
    "source": {
      "$ref": "contentcontainer"
    },
    "with": {
      "properties": {
        "_trackingId": {
          "type": "number",
          "title": "Tracking id",
          "default": 0
        },
        "layoutOptions": {
          "type": "array",
          "title": "Layout options",
          "default": [],
          "_adapt": {
            "editorOnly": true
          }
        }
      }
    }
  }
}
