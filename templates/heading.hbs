{{import_globals}}

<div id="{{_id}}-heading" class="js-heading-inner" {{{a11y_attrs_heading _type}}}>

  <span class="aria-label">
  {{#if _isA11yCompletionDescriptionEnabled}}
    {{#if _isOptional}}
      {{{compile title}}}
      {{else if _isComplete}}
      {{{compile title}}} ({{_type}} {{_globals._accessibility._ariaLabels.complete}})
      {{else}}
      {{{compile title}}} ({{_type}} {{_globals._accessibility._ariaLabels.incomplete}})
    {{/if}}
    {{else}}
    {{{compile title}}}
  {{/if}}
  </span>

</div>
